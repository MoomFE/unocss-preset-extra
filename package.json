{
  "name": "unocss-preset-extra",
  "version": "0.4.3",
  "description": "",
  "keywords": [
    "css",
    "unocss",
    "unocss-preset",
    "unocss-preset-extra",
    "animate.css"
  ],
  "license": "MIT",
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "homepage": "https://github.com/MoomFE/unocss-preset-extra#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoomFE/unocss-preset-extra.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/unocss-preset-extra/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "pre-task": "pnpm install && tsx scripts/transitionAnimateCSS.ts",
    "build": "pnpm install && npm run pre-task && tsx scripts/build.ts",
    "test": "pnpm install && npm run pre-task && vitest",
    "test:ui": "pnpm install && npm run pre-task && vitest --ui",
    "test-build": "pnpm install && npm run build && vitest --mode test-build",
    "test-build:ui": "pnpm install && npm run build && vitest --mode test-build --ui",
    "lint": "eslint ./src --fix"
  },
  "peerDependencies": {
    "unocss": ">= 0.31.0 < 1"
  },
  "devDependencies": {
    "@moomfe/eslint-config": "^2.2.2",
    "@moomfe/small-utils": "^5.3.1",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/postcss-js": "^4.0.0",
    "@unocss/autocomplete": "^0.49.0",
    "@vitest/ui": "^0.28.2",
    "animate.css": "^4.1.1",
    "cssnano": "^5.1.14",
    "eslint": "^8.32.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "local-pkg": "^0.4.3",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.21",
    "postcss-js": "^4.0.0",
    "rollup": "^3.10.1",
    "rollup-plugin-dts": "^5.1.1",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "unocss": "^0.49.0",
    "vite": "^4.0.4",
    "vitest": "0.28.2"
  }
}
