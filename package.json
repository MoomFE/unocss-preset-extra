    "pnpm": "^9.15.9",
    "postcss": "^8.5.3",
    "postcss-js": "^4.0.1",
    "rollup": "^4.40.1",
    "rollup-plugin-dts": "^6.2.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
