{
  "name": "unocss-preset-extra",
  "version": "0.4.3",
  "description": "",
  "keywords": [
    "css",
    "unocss",
    "unocss-preset",
    "unocss-preset-extra",
    "animate.css"
  ],
  "license": "MIT",
  "author": "Wei Zhang <https://github.com/Zhang-Wei-666>",
  "homepage": "https://github.com/MoomFE/unocss-preset-extra#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoomFE/unocss-preset-extra.git"
  },
  "bugs": {
    "url": "https://github.com/MoomFE/unocss-preset-extra/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "pre-task": "pnpm install && tsx scripts/transitionAnimateCSS.ts",
    "build": "pnpm install && npm run pre-task && tsx scripts/build.ts",
    "test": "pnpm install && npm run pre-task && vitest",
    "test:ui": "pnpm install && npm run pre-task && vitest --ui",
    "test-build": "pnpm install && npm run build && vitest --mode test-build",
    "test-build:ui": "pnpm install && npm run build && vitest --mode test-build --ui",
    "lint": "eslint ./src --fix"
  },
  "peerDependencies": {
    "unocss": ">= 0.31.0 < 1"
  },
  "devDependencies": {
    "@moomfe/eslint-config": "^2.1.2",
    "@moomfe/small-utils": "^5.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.15",
    "@types/postcss-js": "^4.0.0",
    "@unocss/autocomplete": "^0.45.18",
    "@vitest/ui": "^0.23.1",
    "animate.css": "^4.1.1",
    "cssnano": "^5.1.13",
    "eslint": "^8.23.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "local-pkg": "^0.4.2",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.16",
    "postcss-js": "^4.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "tsx": "^3.9.0",
    "typescript": "^4.8.2",
    "unocss": "^0.45.18",
    "vite": "^3.1.0",
    "vitest": "0.23.1"
  }
}
