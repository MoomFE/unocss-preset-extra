    "animate.css": "^4.1.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "local-pkg": "^0.5.0",
    "lodash-es": "^4.17.21",
    "mixte": "^1.6.0",
    "pnpm": "^8.9.2",
    "postcss": "^8.4.31",
    "postcss-js": "^4.0.1",
    "rollup": "^4.1.4",
    "rollup-plugin-dts": "^6.1.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.1",
    "vite": "^4.5.0",
    "vitest": "0.34.6"
  }
